ARCH_DIR = arch/arm

ARCH_CPPFLAGS = \
	-DCONFIG_ARM \
	-D__ARM__ \
	-marm \
	-mno-thumb-interwork \
	-ffixed-r8 \
	-I$(ARCH_DIR)/include

ARCH_OBJS = \
    $(ARCH_DIR)/lib/_ashldi3.o \
    $(ARCH_DIR)/lib/_ashrdi3.o \
    $(ARCH_DIR)/lib/_divsi3.o \
    $(ARCH_DIR)/lib/_lshrdi3.o \
    $(ARCH_DIR)/lib/_modsi3.o \
    $(ARCH_DIR)/lib/_udivsi3.o \
    $(ARCH_DIR)/lib/_umodsi3.o \
    $(ARCH_DIR)/lib/bootm.o \
    $(ARCH_DIR)/lib/cache-cp15.o \
    $(ARCH_DIR)/lib/cache.o \
    $(ARCH_DIR)/lib/div0.o \
    $(ARCH_DIR)/lib/eabi_compat.o \
    $(ARCH_DIR)/lib/interrupts.o \
    $(ARCH_DIR)/lib/reset.o

